
#include<iostream>
using namespace std;
int power(int a,int b)
{
    int ans=1;
    for(int i=1;i<=b;i++)
    {
        ans=ans*a;
    }
    return ans;
}
int main() {
    int m,n,ans;
  cout<<"Enter the number and the power\n";
    cin>>m>>n;
    ans=power(m,n);
    cout<<"Answer is "<<ans<<endl;
    return 0;
}
