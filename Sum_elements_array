#include <iostream>
using namespace std;
int main() {
    int n;
    cout << "Enter the size of the array\n";
    cin >> n;
    cout<<"Enter the elements of the array\n";
    int sum=0;
    int arr[100];
    for(int i=0;i<n;i++)
    {
        cin >> arr[i];
        sum=sum+arr[i];
    }
    cout<<"Sum of the elements is : " << sum<<endl;
    return 0;
}
